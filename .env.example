# Database (Aurora PostgreSQL via RDS Proxy)
DB_HOST=your-rds-proxy-endpoint.proxy-xxx.us-east-2.rds.amazonaws.com
DB_PORT=5432
DB_NAME=forecast
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_SSLMODE=require

# AWS Bedrock (uses EC2 IAM role â€” no keys needed)
AWS_REGION=us-east-2
BEDROCK_MODEL_ID=us.amazon.nova-pro-v1:0

# Query limits
MAX_QUERY_ROWS=5000
QUERY_TIMEOUT_SEC=120
